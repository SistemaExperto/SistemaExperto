<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Controlador</servlet-name>
        <servlet-class>Controlador.Controlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Validar</servlet-name>
        <servlet-class>Controlador.Validar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorUsuario</servlet-name>
        <servlet-class>Controlador.ControladorUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorFamiliar</servlet-name>
        <servlet-class>Controlador.ControladorFamiliar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorVivienda</servlet-name>
        <servlet-class>Controlador.ControladorVivienda</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorBD</servlet-name>
        <servlet-class>Controlador.ControladorBD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorGeneral</servlet-name>
        <servlet-class>Controlador.ControladorGeneral</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorReporte</servlet-name>
        <servlet-class>Controlador.ControladorReporte</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Controlador</servlet-name>
        <url-pattern>/Controlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Validar</servlet-name>
        <url-pattern>/Validar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorUsuario</servlet-name>
        <url-pattern>/ControladorUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorFamiliar</servlet-name>
        <url-pattern>/ControladorFamiliar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorVivienda</servlet-name>
        <url-pattern>/ControladorVivienda</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorBD</servlet-name>
        <url-pattern>/ControladorBD</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorGeneral</servlet-name>
        <url-pattern>/ControladorGeneral</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorReporte</servlet-name>
        <url-pattern>/ControladorReporte</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
